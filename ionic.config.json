{
  "name": "staticTodo",
  "integrations": {
    "cordova": {}
  },
  "type": "ionic1",
  "watchPatterns": [
    "scss/**/*",
    "www/**/*",
    "!www/lib/**/*",
    "!www/**/*.map"
  ],
  "proxies": [
    {
      "path": "/addUser",
      "proxyUrl": "https://appliwebpolytech.herokuapp.com/addUser"
    },
    {
      "path": "/Connect",
      "proxyUrl": "https://appliwebpolytech.herokuapp.com/Connect"
    },
    {
      "path": "/getListSet",
      "proxyUrl": "https://appliwebpolytech.herokuapp.com/getListSet"
    },
    {
      "path": "/creerListe",
      "proxyUrl": "https://appliwebpolytech.herokuapp.com/creerListe"
    },
    {
      "path": "/getTaskSet",
      "proxyUrl": "https://appliwebpolytech.herokuapp.com/getTaskSet"
    },
    {
      "path": "/addTask",
      "proxyUrl": "https://appliwebpolytech.herokuapp.com/addTask"
    },
    {
      "path": "/delTask",
      "proxyUrl": "https://appliwebpolytech.herokuapp.com/delTask"
    }
  ]
}
